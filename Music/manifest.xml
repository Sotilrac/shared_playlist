<?xml version='1.0' encoding='UTF-8'?>
<package version="1.5.4" uuid="music">
 <names>
  <name lang="en_US">Music</name>
 </names>
 <descriptions>
  <description lang="en_US">Naoqi service allowing to fetch music from the internet and play it.

Go to {robot adress}/apps/music to use the web interface.
</description>
 </descriptions>
 <supportedLanguages>
  <language>en_US</language>
 </supportedLanguages>
 <descriptionLanguages>
  <language>en_US</language>
 </descriptionLanguages>
 <contents>
  <behaviorContent path=".">
   <userRequestable/>
   <nature>interactive</nature>
   <triggerSentences>
    <sentence lang="en_US">Music</sentence>
    <sentence lang="en_US">Jukebox</sentence>
   </triggerSentences>
   <loadingResponses/>
   <autonomous/>
   <permissions/>
  </behaviorContent>
 </contents>
 <requirements>
  <naoqiRequirement minVersion="2.1.0.19"/>
  <robotRequirement model="JULIETTE"/>
  <robotRequirement model="NAO"/>
 </requirements>
 <services>
  <service name="Music" autorun="true" execStart="/usr/bin/python2 lib/register_service.py"/>
 </services>
</package>
